#JSGF V1.0;
grammar Exam_Details;

public <a> = [<greeting>] what <is> [the] [final] <exam> <detail>;
public <b> = [<greeting>] [tell] [me] [about] [final] <exam> <detail>;
public <c> = [<greeting>] when is the [final] exam;


<is> = is | are;
<greeting> = hi | hey | hello;
<detail> = detail | details | about | on;
<exam> = exam | test| paper;




