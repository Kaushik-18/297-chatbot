#JSGF V1.0;
grammar Professor_Office_Location;

public <a> = [<greeting>] where is [the] <noun> <room>;
public <c> = [<greeting>] [please] tell (me|us) [the] <noun> <room>;

<greeting> = hi | hey | hello; 
<room> = office | room | location | building;
<grammer> = sit | located | situated;
<noun> = sir| professor  | teacher;


