#JSGF V1.0;
grammar Negative_Professor_Office_Location;

public <a> = [<greeting>] where is [the] <noun> (<room>);
public <b> = [<greeting>] where [does] [the] <noun> <grammer>;
public <c> = [<greeting>] what is the <room> of [the] <noun>;
public <d> = [<greeting>] [tell] [me] [the] <noun> (<room>);

<greeting> = hi | hey | hello; 
<room> = office | room | location | building;
<grammer> = sit | located | situated;
<noun> = doctor | actor | player;



